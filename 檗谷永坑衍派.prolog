% [1:子田,2:隆敏,3:肇基,4:菊崑,5:肖業,6:梅鰲,7:振魏,8:國軒,9:景遠,10:知履,11:挺亮,12:際賢,13:鍾珀,14:英普,15:毓雲,16:秀淡,17:科,18:境]
% Anc = [1:子田, 2:隆敏, 3:肇基, 4:菊崑, 5:肖業, 6:梅鰲, 7:振魏, 8:國軒, ... : ...|...] ;
% false.

alt_name(Gen:Name, Nm) :-
    ( n(Gen:Name, Nm), !
    ; n(Gen:Nm, Name), !
    ; cn(Gen:Name, Nm), !
    ; cn(Gen:Nm, Name), !
    ; ps(Gen:Name, Nm), !
    ; ps(Gen:Nm, Name)
    ).

parent_children(Parent, Children) :-
    pc(_, Parent, _, Children).

name(Key, Name) :-
    n(Key, Name).

courtesy_name(Key, Name) :-
    cn(Key, Name).

pseudonym(Key, Name) :-
    ps(Key, Name).

