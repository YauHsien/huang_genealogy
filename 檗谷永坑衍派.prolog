:- encoding(utf8).
:- module(family_tree,
          [ ancestors/2,
            parent_children/2,
            name/2,
            courtesy_name/2,
            pseudonym/2 ]).
% 世系：以永坑第一世為 1 ，第二世為 2 等等。
%      檗谷世系以龍公為太乙，因此若依檗谷世系，永坑每一世須加 2 。
%      台開基以十三世鍾珀公為開基祖，則永坑每一世須減 12 。
% 斷句： parent_children(世別:父諱名, 世別:[長子諱名, 次子諱名, ... ]).
%       name(世別:諱名, 名).
%       courtesy_name(世別:諱名, 字).
%       pseudonym(世別:諱名, 號).

head(1:子田).

ancestors(Name, Ancestors) :-
    ( gen(Name, Gen), !,
      Nm = Name
    ; alt_name(Gen:Name, Nm)
    ),
    parent(P, Gen:Nm, _, _),
    ancestors(Gen:Nm, P, [], Ancestors).

gen(Name, Gen) :-
    pc(_, _, _, Gen:Children),
    member(Name, Children).

parent(P, Gen:Name, P2, GenP:NameP) :-
    pc(P2, GenP:NameP, P, Gen:Cl),
    member(Name, Cl).

ancestors(Gen:Name, _P, Acc, Acc) :-
    head(Gen:Name), !.
ancestors(Gen:Name, P, Acc, Children) :-
    ( gen(Name, Gen), !,
      Nm = Name
    ; alt_name(Gen:Name, Nm)
    ),
    parent(P, Gen:Nm, P2, GenP:NameP),
    ancestors(GenP:NameP, P2, [GenP:NameP|Acc], Children).
